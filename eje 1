class Animal:
    def hacer_ruido(self):
        print("Ruido generico de animal")

class Perro(Animal):
    def hacer_ruido(self):
        print("Guau Guau")

class Gato(Animal):
    def hacer_ruido(self):
        print("Miau Miau")
        
animal_generico = Animal()
perro = Perro()
gato = Gato()

animal_generico.hacer_ruido()
perro.hacer_ruido()
gato.hacer_ruido()
